
@{
    ViewData["Title"] = "Student Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Sidebar {
    <ul>
        <li><a class="sb-link">Punchclock</a></li>
        <li><a class="sb-link">Lectures</a></li>
        <li><a class="sb-link">Excersies</a></li>
        <li><a class="sb-link">Course Calendar</a></li>
